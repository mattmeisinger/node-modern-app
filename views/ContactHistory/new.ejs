<form action="/contactHistory/create" method="POST" role="form" class="new">
  <div class="form-group">
    <h2>Create a contactHistory</h2>

    <% if(flash && flash.err) { %>
      <ul class="alert alert-success">
        <% Object.keys(flash.err).forEach(function(error) { %>
          <li><%- JSON.stringify(flash.err[error]) %></li>
        <% }) %>
      </ul>
    <% } %>

    <div class="control-group" style = "width: 300px;">
      <input type="text" class="form-control" placeholder="data" name="data">
    </div>

    <div class="control-group" style = "width: 300px;">
      <input type="text" class="form-control" placeholder="model" name="model">
    </div>

    <div class="control-group" style = "width: 300px;">
      <input type="text" class="form-control" placeholder="summary" name="summary">
    </div>

    <div class="form-group" style="width: 400px;">
      <label>customer</label>

      <div>
        <select name="customer" class="form-control">
          <% for(var i=0; i<customers.length; i++) {%>
          <option value="<%= customers[i].id %>" ><%= customers[i].firstName + ' ' + customers[i].lastName %></option>
          <% } %>
        </select>
      </div>

    </div>   

    <div class="form-group" style="width: 400px;">
      <label>agent</label>

      <div>
        <select name="agent" class="form-control">
          <% for(var i=0; i<agents.length; i++) {%>
          <option value="<%= agents[i].id %>" ><%= agents[i].firstName + ' ' + agents[i].lastName %></option>
          <% } %>
        </select>
      </div>

    </div>

  </div>

  <input type="submit" value="Submit" class="btn btn-default"/> |
  <a href="/contactHistory">Back to list</a>

</form>
