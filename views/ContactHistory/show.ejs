<form class="show">
  <div class="form-group">
    <h2> a contactHistory... </h2>

    <div class="form-group" style="width: 400px;">
      <label>data</label>
      <input value="<%= contactHistory.data %>" name="data" type="text" class="form-control" disabled/>
    </div>

    <div class="form-group" style="width: 400px;">
      <label>model</label>
      <input value="<%= contactHistory.model %>" name="model" type="text" class="form-control" disabled/>
    </div>

    <div class="form-group" style="width: 400px;">
      <label>summary</label>
      <input value="<%= contactHistory.summary %>" name="summary" type="text" class="form-control" disabled/>
    </div>

    <div class="form-group" style="width: 400px;">
      <label>customer</label>
      <input value="<%= customer.firstName + ' ' + customer.lastName %>" name="customer" type="text" class="form-control" disabled/>
    </div>

    <div class="form-group" style="width: 400px;">
      <label>agent</label>
      <input value="<%= agent.firstName + ' ' + agent.lastName %>" name="agent" type="text" class="form-control" disabled/>
    </div>

  </div>
</div>

<a href="/contactHistory/edit/<%=contactHistory.id %>" class="btn btn-default">Edit</a> |
<a href="/contactHistory">Back to list</a>

</form>
